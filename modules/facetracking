#!/usr/bin/env bash  
set -euo pipefail  
  
# Start baballonia-uvc detached  
nohup baballonia-uvc >/tmp/baballonia-uvc.log 2>&1 &  
  
# Start VRCFaceTracking.Avalonia detached  
nohup vrcft-avalonia >/tmp/vrcft-avalonia.log 2>&1 &  
  
# Script can exit immediately; both processes keep running  
exit 0   
